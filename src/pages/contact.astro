---
import Page from "~layouts/Page.astro";

import banner from "~assets/banner/contact.jpg";

import { Content as Content } from "~content/contact.mdx";
import "~styles/markdown.scss";

const title = "Contact | Gents & Ginger";
const description = "Get in touch with us for bookings, collaborations, or general inquiries.";
---

<Page title={title} description={description} bannerimage={banner} bannertext="Contact">
    <article>
        <Content />
    </article>
</Page>
